+    // MARK: - 刘海触发设置
+    func setupNotchTrigger() {
+        let defaults = UserDefaults.standard
+        let coordinator = NotchHoverCoordinator(
+            triggerWidthRatio: 0.15,
+            showDelay: defaults.double(forKey: DefaultsKey.hoverShowDelay),
